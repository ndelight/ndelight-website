{
    "rewrites": [
        {
            "source": "/events/:match*",
            "destination": "/event.html"
        },
        {
            "source": "/book/:match*",
            "destination": "/book.html"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x",
            "memory": 1024,
            "maxDuration": 10
        }
    }
}